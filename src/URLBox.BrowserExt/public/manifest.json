{
  "author": "chet",
  "description": "收录你喜欢的网址",
  "icons":
    {
      "48": "icons/box-48.png",
      "96": "icons/box-96.png"
    },
  "manifest_version": 2,
  "name": "urlbox",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "30": "icons/box-30.png"
    },
    "default_title": "urlbox",
    "default_popup": "index.html"
  }
}